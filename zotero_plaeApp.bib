	author = {Laboissonniere, Lauren A. and Goetz, Jillian J. and Martin, Gregory M. and Bi, Ran and Lund, Terry J. S. and Ellson, Laura and Lynch, Madison R. and Mooney, Bailey and Wickham, Hannah and Liu, Peng and Schwartz, Gregory W. and Trimarchi, Jeffrey M.},
	month = oct,
	year = {2019},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Retina, Transcriptomics},
	pages = {15778},
	file = {Full Text PDF:/Users/mcgaugheyd/Zotero/storage/656AIDPN/Laboissonniere et al. - 2019 - Molecular signatures of retinal ganglion cells rev.pdf:application/pdf},
}

@article{mizeracka_analysis_2013,
	title = {Analysis of gene expression in wild-type and {Notch1} mutant retinal cells by single cell profiling},
